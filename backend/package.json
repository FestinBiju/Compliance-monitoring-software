{
  "name": "compliance-backend",
  "version": "0.1.0",
  "scripts": {
    "install": "pip install -r requirements.txt",
    "dev": "python -m uvicorn app.main:app --reload --port 8000",
    "start": "python -m uvicorn app.main:app --host 0.0.0.0 --port 8000",
    "migrate:create": "python -m alembic revision --autogenerate -m",
    "migrate:up": "python -m alembic upgrade head",
    "migrate:down": "python -m alembic downgrade -1"
  }
}
